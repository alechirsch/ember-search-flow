{
  "name": "ember-search-flow",
  "version": "4.0.0",
  "description": "Multi-faceted search for ember.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "repository": "https://github.com/alechirsch/ember-search-flow",
  "engines": {
    "node": ">= 18.0.0"
  },
  "author": "Alec Hirsch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alechirsch/ember-search-flow/issues"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@ember/optional-features": "^2.1.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.1",
    "@embroider/test-setup": "^3.0.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.7.0",
    "ember-cli": "~5.4.1",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~5.3.0",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.13.0",
    "ember-template-lint-plugin-prettier": "^5.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^11.12.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^3.1.1",
    "qunit": "^2.20.0",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.89.0"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.7",
    "ember-cli-autoprefixer": "^2.0.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-sass": "^11.0.1",
    "ember-get-config": "^2.1.1",
    "ember-modifier": "^4.1.0"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions"
  ],
  "keywords": [
    "ember-addon",
    "visual search",
    "search",
    "facet",
    "mulit-facet",
    "search flow",
    "query",
    "filter",
    "filters"
  ],
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://alechirsch.github.io/ember-search-flow/"
  }
}
